{
  "name": "mastodon-openapi",
  "version": "1.0.0",
  "description": "Mastodon OpenAPI package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "generate": "ts-node src/generate.ts",
    "validate": "validate-api dist/schema.json && redocly lint --format=github-actions dist/schema.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "ts-node scripts/setup-docs.ts",
    "update-docs-sha": "ts-node scripts/update-docs-sha.ts"
  },
  "keywords": [
    "mastodon",
    "openapi",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@redocly/cli": "^1.34.3",
    "@seriousme/openapi-schema-validator": "^2.4.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^30.0.3",
    "prettier": "^3.6.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "gray-matter": "^4.0.3"
  }
}
